{% comment %} {% for link in linklists.main-menu.links %}
  {% assign child_list_handle = link.title | handleize %}
  {% if linklists[child_list_handle].links != blank %}
    <a href="{{ link.url }}">{{ link.title }}</a>
    [{% for childlink in linklists[child_list_handle].links %}
      <a href="{{ childlink.url }}">{{ childlink.title | escape }}</a>
    {% endfor %}]
  {% else %}
    <a href="{{ link.url }}">{{ link.title }}</a>
  {% endif %}
{% endfor %}

<a href="/cart">cart</a>

{% if shop.customer_accounts_enabled %}
  {% if customer %}
    <a href="/account">account</a>
    {{ 'log out'  | customer_logout_link }}
  {% else %}
    {{ 'log in ' | customer_login_link }}
    {{ 'register' | customer_register_link }}
  {% endif %}
{% endif %} {% endcomment %}

<header class="header" id="header">

  <div class="header-inner flex space-between">
  
    <div class="menu-drawer" style="display:none;">

    </div>

    <a href="/" class="header-title">{{ shop.name }}</a>

    <div class="cart-button-container">
    
    </div>

    <div class="mini-cart" style="display:none;">
    
    </div>

  </div>
  
</header>